{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "algolia/algoliasearch-client-php": "^1.25",
        "algolia/laravel-scout-algolia-macros": "^0.5.0",
        "algolia/laravel-scout-settings": "^2.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "csun-metalab/laravel-directory-authentication": "^1.5",
        "csun-metalab/laravel-proxypass": "^1.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "google/recaptcha": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "2.*",
        "itsgoingd/clockwork": "~1.12",
        "laravel/framework": "5.6.*",
        "laravel/scout": "^4.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "5.6.*",
        "tiesa/ldap": "dev-master",
        "tom-lingham/searchy": "2.*"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "symfony/css-selector": "~4.0",
        "symfony/dom-crawler": "~4.0",
        "friendsofphp/php-cs-fixer": "^2.2.0",
        "refinery29/php-cs-fixer-config": "^0.10.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Models",
            "app/Auth",
            "app/Mailers"
        ],
        "psr-4": {
            "Helix\\": "app/",
            "Tests\\": "tests/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
